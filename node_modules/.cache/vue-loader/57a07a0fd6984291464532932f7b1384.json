 >\n      <h2 class=\"text-lg mr-auto font-bold\">Detail skill</h2>\n      <!-- <v-select\n        class=\"border-gray-300 w-2/5\"\n        :options=\"optionsSelect\"\n        v-model=\"onlanguagechange\"\n        :clearable=\"false\"\n      ></v-select> -->\n      <tree-select\n        :options=\"option\"\n        v-model=\"onlanguagechange\"\n        :normalizer=\"normalizer\"\n      />\n    </div>\n    <div class=\"p-2 pt-5 overflow-y-auto bg-white h-80\">\n      <div class=\"char-detail-skill\">\n        <ApexCharts\n          type=\"bar\"\n          height=\"270\"\n          :options=\"chartOptions\"\n          :series=\"series\"\n        ></ApexCharts>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport api from \"@/api/api\";\nexport default {\n  data() {\n    return {\n      series: [\n        {\n          data: [10, 20, 30, 40, 50],\n        },\n      ],\n      chartOptions: {\n        chart: {\n          type: \"bar\",\n          height: 280,\n          stacked: true,\n          toolbar: {\n            show: false,\n          },\n        },\n        plotOptions: {\n          bar: {\n            barHeight: \"100%\",\n            distributed: true,\n            horizontal: true,\n            dataLabels: {\n              position: \"bottom\",\n            },\n          },\n        },\n        dataLabels: {\n          enabled: true,\n          textAnchor: \"start\",\n          style: {\n            colors: [\"#fff\"],\n          },\n          formatter: function (val, opt) {\n            return opt.w.globals.labels[opt.dataPointIndex] + \":  \" + val;\n          },\n          offsetX: 0,\n          dropShadow: {\n            enabled: true,\n          },\n        },\n        stroke: {\n          width: 1,\n          colors: [\"#fff\"],\n        },\n        fill: {\n          opacity: 1,\n        },\n        colors: [\"#003993\", \"#007BC3\", \"#22AEE5\", \"#45D1D1\", \"#F8BF2D\"],\n        xaxis: {\n          categories: [\"S\", \"A\", \"B\", \"C\", \"D\"],\n        },\n        yaxis: {\n          labels: {\n            show: false,\n          },\n        },\n        tooltip: {\n          theme: \"dark\",\n          x: {\n            show: false,\n          },\n          y: {\n            title: {\n              formatter: function () {\n                return \"\";\n              },\n            },\n          },\n        },\n      },\n      optionsSelect: [],\n      onlanguagechange: null,\n    };\n  },\n  created() {\n    this.getCategoryAll();\n    // this.getDetailSkill();\n  },\n  methods: {\n    normalizer(node) {\n      return {\n        id: node.key,\n        label: node.name,\n        children: node.subOptions,\n      };\n    },\n    transferListOption(array) {\n      const optionsClone = [];\n      array.map((item) => {\n        const newOption = {};\n        newOption.id = item.id;\n        newOption.label = item.name;\n        optionsClone.push(newOption);\n      });\n      return optionsClone;\n    },\n    async getCategoryAll() {\n      const res = await api(\"getCategoryAll\");\n      const Options = res.data.data.map((category) => {\n        const option = {};\n        option.id = category.id;\n        console.log(option);\n        option.label = category.name;\n        option.children =\n          category?.listSubCategories &&\n          category.listSubCategories.map((subCategory) => {\n            const subOption = {};\n            subOption.id = subCategory.id;\n            subOption.label = subCategory.name;\n            subOption.children =\n              subCategory?.listSubCategories &&\n              subCategory.listDetailSubCategories.map((detailCategory) => ({\n                id: detailCategory.id,\n                label: detailCategory.name,\n              }));\n            return subOption;\n          });\n        return option;\n      });\n      console.log(Options);\n      const listOptions = this.transferListOption(\n        res.data.data[0].listSubCategories\n      );\n      this.optionsSelect = [...listOptions];\n      this.onlanguagechange = this.optionsSelect[0];\n      const data = await api(\"getDetailSkill\", {\n        id: this.onlanguagechange.id,\n      });\n      console.log(listOptions);\n      console.log(this.optionsSelect[0]);\n      return res.data.data[0].listSubCategories;\n    },\n  },\n};\n</script>\n<style scoped></style>\n"]}]}