n    <div class=\"p-2 pt-5 overflow-y-auto bg-white h-80\">\n      <ApexCharts\n        type=\"bar\"\n        width=\"440\"\n        :options=\"options\"\n        :series=\"series\"\n      ></ApexCharts>\n    </div>\n  </div>\n</template>\n<script>\nimport api from \"@/api/api\";\nexport default {\n  data() {\n    return {\n      series: [\n        {\n          name: [\"Program Language point\"],\n          data: [11, 22, 44, 33, 22],\n        },\n      ],\n      optionsSelect: [],\n      onlanguagechange: null,\n      options: {\n        chart: {\n          toolbar: {\n            show: false,\n          },\n          height: 350,\n          events: {\n            click: function (chart, w, e) {\n              // console.log(chart, w, e)\n            },\n          },\n          zoom: {\n            enabled: false,\n          },\n        },\n        colors: [\"#003993\", \"#007BC3\", \"#22AEE5\", \"#45D1D1\", \"#F8BF2D\"],\n        plotOptions: {\n          bar: {\n            columnWidth: \"50%\",\n            distributed: true,\n            dataLabels: {\n              position: \"top\",\n            },\n          },\n        },\n        dataLabels: {\n          enabled: true,\n        },\n        legend: {\n          show: false,\n        },\n        xaxis: {\n          categories: [\n            [\"Lê Hồng\", \"Minh\"],\n            [\"Đào Quang\", \"Linh\"],\n            [\"Nguyễn Thị\", \"Nhân\"],\n            [\"Vũ Bình\", \"Minh\"],\n            [\"Trần Đức\", \"Anh\"],\n          ],\n          labels: {\n            style: {\n              colors: [\"#003993\", \"#007BC3\", \"#22AEE5\", \"#45D1D1\", \"#F8BF2D\"],\n              fontSize: \"12px\",\n            },\n          },\n        },\n        yaxis: {\n          show: false,\n        },\n      },\n    };\n  },\n  created() {\n    this.listCategories();\n  },\n  methods: {\n    transferListOption(array) {\n      const optionsClone = [];\n      array.forEach((item) => {\n        const newOption = {};\n        newOption.id = item.id;\n        newOption.label = item.name;\n        optionsClone.push(newOption);\n      });\n      return optionsClone;\n    },\n    async listCategories() {\n      const res = await api(\"getListCategories\");\n      const listOptions = this.transferListOption(res.data.data);\n      this.optionsSelect = [...listOptions];\n      this.onlanguagechange = this.optionsSelect[0];\n    },\n  },\n};\n</script>\n<style scoped></style>\n"]}]}